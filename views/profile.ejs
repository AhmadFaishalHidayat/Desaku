<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>profile</title>
  </head>
  <body>
    <h1>HALAMAN profile <%= data[0].username %></h1>
    <a href="/"><button>kembali</button></a>
    <a href="/beranda"><button>BERANDA</button></a>
    <a href="/logout"><button>logout</button></a>
    <% data.forEach(user => { %> <% user.Posts.forEach(post => { %>
    <div class="card">
      <img
        src="<%= post.imgUrl %>"
        alt="Avatar"
        style="width: 300px; height: 300px"
      />
      <div class="container">
        <h4><b><%= post.title %></b></h4>
        <p><%= post.content %></p>
      </div>
    </div>
    <% }) %> <% }) %>
  </body>
</html>
